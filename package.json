{
    "name": "mh-site",
    "version": "1.0.0",
    "license": "MIT",
    "devDependencies": {
        "@11ty/eleventy": "2.0.0-canary.16",
        "@11ty/eleventy-cache-assets": "^2.3.0",
        "@11ty/eleventy-img": "^2.0.1",
        "@11ty/eleventy-navigation": "^0.3.5",
        "@11ty/eleventy-plugin-directory-output": "^1.0.1",
        "@11ty/eleventy-plugin-rss": "^1.2.0",
        "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
        "@11ty/eleventy-plugin-webc": "^0.4.0",
        "@netlify/plugin-lighthouse": "^3.7.1",
        "@tailwindcss/typography": "^0.5.7",
        "autoprefixer": "^10.4.12",
        "cssnano": "^5.1.13",
        "eleventy-plugin-reading-time": "^0.0.1",
        "eleventy-plugin-youtube-embed": "^1.7.1",
        "html-minifier": "^4.0.0",
        "markdown-it": "^13.0.1",
        "markdown-it-anchor": "^8.6.5",
        "markdown-it-for-inline": "^0.1.1",
        "netlify-plugin-cache": "^1.0.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.17",
        "postcss-cli": "^10.0.0",
        "postcss-import": "^15.0.0",
        "prettier": "^2.7.1",
        "tailwindcss": "^3.1.8",
        "tailwindcss-debug-screens": "^2.2.1"
    },
    "scripts": {
        "dev:twnd": "env ELEVENTY_ENV=development postcss src/_css/tailwind.css -o dist/assets/css/theme.css --watch --verbose",
        "dev:11ty": "env ELEVENTY_ENV=development eleventy --serve",
        "dev": "npm-run-all clean --parallel dev:* --print-label",
        "build:twnd": "env ELEVENTY_ENV=production postcss src/_css/tailwind.css -o dist/assets/css/theme.css",
        "build:11ty": "env ELEVENTY_ENV=production eleventy",
        "build": "npm-run-all build:* --print-label",
        "format": "prettier --write \"src/**/*.{js,json,md}\"",
        "clean": "rm -rf ./dist"
    },
    "dependencies": {
        "@11ty/is-land": "^2.0.3",
        "speedlify-score": "^2.0.2"
    }
}
