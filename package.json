{
    "name": "mh-site",
    "version": "1.0.0",
    "license": "MIT",
    "devDependencies": {
        "@11ty/eleventy": "^3.1.1",
        "@11ty/eleventy-img": "latest",
        "@11ty/eleventy-navigation": "latest",
        "@11ty/eleventy-plugin-rss": "latest",
        "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
        "@11ty/eleventy-plugin-webc": "^0.11.2",
        "@netlify/plugin-lighthouse": "^6.0.0",
        "@tailwindcss/postcss": "latest",
        "@tailwindcss/typography": "latest",
        "cssnano": "latest",
        "dotenv": "latest",
        "eleventy-plugin-reading-time": "latest",
        "eleventy-plugin-youtube-embed": "latest",
        "html-minifier-terser": "^7.2.0",
        "markdown-it": "^14.1.0",
        "markdown-it-anchor": "latest",
        "markdown-it-emoji": "^3.0.0",
        "markdown-it-link-attributes": "^4.0.1",
        "netlify-plugin-cache": "^1.0.3",
        "npm-run-all": "^4.1.5",
        "postcss": "latest",
        "postcss-cli": "latest",
        "prettier": "latest",
        "tailwindcss": "latest",
        "tailwindcss-debug-screens": "latest"
    },
    "scripts": {
        "dev:twnd": "env postcss src/_css/tailwind.css -o dist/assets/css/theme.css --watch --verbose",
        "dev:11ty": "env eleventy --serve",
        "dev": "npm-run-all clean --parallel 'dev:*' --print-label",
        "build:twnd": "env ELEVENTY_ENV=production postcss src/_css/tailwind.css -o dist/assets/css/theme.css",
        "build:11ty": "env ELEVENTY_ENV=production eleventy",
        "build": "npm-run-all 'build:*' --print-label",
        "format": "prettier --write \"{src,content}/**/*.{js,json,css,md}\"",
        "clean": "rm -rf ./dist"
    },
    "dependencies": {
        "@11ty/is-land": "^4.0.0",
        "speedlify-score": "^4.0.4"
    },
    "packageManager": "yarn@3.6.1"
}
